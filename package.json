{
	"name": "sveltkit-starterkit",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"test:ci": "npm run check && npm run test:unit && npm run test:integration",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test --project chromium --project firefox",
		"test:unit": "vitest run --passWithNoTests",
		"prepare": "husky"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.21.0",
		"@fontsource/fira-mono": "^5.2.5",
		"@neoconfetti/svelte": "^2.2.1",
		"@playwright/test": "^1.50.1",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.17.3",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/eslint": "^9.6.1",
		"typescript-eslint": "^8.20.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^10.0.2",
		"eslint-plugin-svelte": "^3.0.2",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"postcss": "^8.5.3",
		"postcss-load-config": "^6.0.1",
		"prettier": "^3.5.2",
		"prettier-plugin-svelte": "^3.3.3",
		"sass": "^1.85.1",
		"svelte": "^5.20.5",
		"svelte-check": "^4.1.4",
		"svelte-intersection-observer": "^1.0.0",
		"svelte-preprocess": "^6.0.3",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"vite": "^6.2.0",
		"vitest": "^3.0.7"
	},
	"type": "module",
	"browserslist": [
		"defaults",
		"last 4 versions",
		"Firefox ESR"
	],
	"dependencies": {
		"@sentry/sveltekit": "^9.3.0",
		"dotenv": "^16.4.7",
		"highlight.js": "^11.11.1",
		"isomorphic-dompurify": "^2.22.0",
		"remixicon": "^4.6.0",
		"slugify": "^1.6.6",
		"sveltekit-sitemap": "github:Joonel/sveltekit-sitemap"
	}
}
